#include <iostream>
#include <string>

using namespace std;

int main() {
    // Конкатенация строк
    string str1 = "Hello";
    string str2 = " World";
    string result = str1 + str2;
    cout << result << endl;  // Выводит "Hello World"

    // Использование оператора +=
    str1 += " C++";
    cout << str1 << endl;  // Выводит "Hello C++"

    // Получение длины строки
    string myString = "Example";
    cout << "Length: " << myString.length() << endl;  // Выводит "Length: 7"
    cout << "Size: " << myString.size() << endl;  // Выводит "Size: 7"

    // Извлечение подстроки
    string original = "Hello, World!";
    string substring = original.substr(7, 5);
    cout << substring << endl;  // Выводит ", Wor"

    // Поиск подстроки
    string searchString = "C++ is fun and C++ is powerful";
    size_t found = searchString.find("C++");
    if (found != string::npos) {
        cout << "Substring found at position " << found << endl;  // Выводит "Substring found at position 0"
    } else {
        cout << "Substring not found" << endl;
    }

    // Замена подстроки
    string replaceString = "C++ is great!";
    replaceString.replace(0, 3, "Java");
    cout << replaceString << endl;  // Выводит "Java is great!"

    return 0;
}
































